@model HomeVM
 <!-- Top start -->
<section id="top-header">
    <div class="top">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-md-6 col-lg-6">
                    <div class="contact">
                        <ul class="d-flex m-0 p-0">
                            <li><i class="fa-solid fa-envelope"></i><a href="#">@Model.Settings.Email</a></li>
                            <li class="ms-5">
                                <i class="fa-solid fa-mobile-screen-button"></i><a href="#">
                                    @Model.Settings.Phone
                                </a>
                            </li>

                        </ul>
                    </div>
                </div>
                <div class=" col-md-6 col-lg-6">
                    <div class="icons">
                        <ul class="d-flex justify-content-end m-0 p-0">
                            @foreach (var item in Model.SocialMedias)
                            {
                <li class="ms-3"><a href="@item.IconUrl"><i class="@item.Icon"></i></a></li>}
                            
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- Top end -->
